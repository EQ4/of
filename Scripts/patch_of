#!/bin/bash

NORMAL=$(tput sgr0)
RED=$(tput setaf 1)
GREEN=$(tput setaf 2)
CYAN=$(tput setaf 6)

FILE=$(readlink -f $0)
DIR=`dirname $FILE`

function apply() {
    echo   ""
    echo   ${CYAN}$1${NORMAL}
    patch  -p1 < Patches/$1
}

cd $DIR
cd ..

apply of_audio_video_disable.patch
apply of_mingw_fix.patch
apply of_glew_include_path_fix.patch
apply of_scroll_feature.patch
apply of_clipboard_feature.patch
apply of_compile_error_fix.patch
apply of_remove_deprecated_registers.patch
apply of_undo_commit_61932bb.patch

